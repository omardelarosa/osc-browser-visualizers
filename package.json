            "sqcr public/sqcr-demo/buffers --config public/sqcr-demo/config/presentation.json --path ."
    },
    "dependencies": {
        "express": "4.16.3",
        "osc": "2.2.1",
        "sqcr": "file:../sqcr",
        "tonal": "^1.1.3",
        "tone": "^0.12.80",
        "vis": "^4.21.0",
        "webmidi": "^2.2.0",
        "ws": "5.1.1"
    }
